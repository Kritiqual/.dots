    "commentBox.styles": {
        "defaultStyle": {
            "capitalize": true,
            "textAlignment": "center",
            "extendSelection": true,
            "boxWidth": 0,
            "removeEmptyLines": false,
            "ignoreOuterIndentation": true,
            "ignoreInnerIndentation": true
        },
        "LuaHead": {
            "boxWidth": 55,
            "commentStartToken": "-------------------------------------------------------\n--/==",
            "commentEndToken": "==/--\n-------------------------------------------------------",
            "topRightToken": "==\\--",
            "bottomLeftToken": "--\\==",
            "topEdgeToken": "=",
            "bottomEdgeToken": "=",
            "leftEdgeToken": "--|> ",
            "rightEdgeToken": " <|--",
            "fillingToken": ""
        },
        "LuaSect": {
            "boxWidth": 55,
            "capitalize": false,
            "commentStartToken": "----",
            "commentEndToken": "----",
            "topRightToken": "----",
            "bottomLeftToken": "----",
            "topEdgeToken": "----",
            "bottomEdgeToken": "----",
            "leftEdgeToken": "----",
            "rightEdgeToken": "----",
            "fillingToken": ""
        },
        "LuaBreak": {
            "boxWidth": 1,
            "capitalize": false,
            "commentStartToken": "--<",
            "commentEndToken": ">--",
            "topRightToken": "",
            "bottomLeftToken": "",
            "topEdgeToken": "",
            "bottomEdgeToken": "",
            "leftEdgeToken": "",
            "rightEdgeToken": "",
            "fillingToken": ""
        },
        "Head": {
            "boxWidth": 55,
            "commentStartToken": "#######################################################\n#/==",
            "commentEndToken": "==/#\n#######################################################",
            "topRightToken": "==\\#",
            "bottomLeftToken": "#\\==",
            "topEdgeToken": "=",
            "bottomEdgeToken": "=",
            "leftEdgeToken": "#|> ",
            "rightEdgeToken": " <|#",
            "fillingToken": ""
        },
        "Sect": {
            "boxWidth": 47,
            "capitalize": false,
            "textAlignment": "left",
            "commentStartToken": "##-----",
            "commentEndToken": "-",
            "topRightToken": "",
            "bottomLeftToken": "",
            "topEdgeToken": "",
            "bottomEdgeToken": "",
            "leftEdgeToken": "",
            "rightEdgeToken": "",
            "fillingToken": "--"
        },
        "Break": {
            "boxWidth": 1,
            "capitalize": false,
            "commentStartToken": "#<<",
            "commentEndToken": ">>",
            "topRightToken": "",
            "bottomLeftToken": "",
            "topEdgeToken": "",
            "bottomEdgeToken": "",
            "leftEdgeToken": "",
            "rightEdgeToken": "",
            "fillingToken": ""
        }
    },
